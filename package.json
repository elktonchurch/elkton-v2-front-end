{
    "name": "elkton-v2-front-end",
    "description": "This is v2.0 of the front-end repo for Elkton Missionary Church's official website.",
    "version": "0.1.0",
    "repository": "https://github.com/elktonchurch/elkton-v2-front-end",
    "author": "Tico S. Thepsourinthone",
    "contributors": [],
    "scripts": {
        "start:dev": "nodemon",
        "build": "next build && tsc --project tsconfig.server.json",
        "start": "cross-env NODE_ENV=production node dist/index.js",
        "test": "jest --coverage",
        "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
        "pretty": "prettier --write --config .prettierrc.js {src,pages,server}/**/*.{ts,tsx}",
        "format": "npm run lint -- --fix",
        "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
        "export": "cross-env NODE_ENV=production next export",
        "cli": "pankod-cli add",
        "storybook": "start-storybook -s ./public/static -p 6006",
        "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout"
    },
    "dependencies": {
